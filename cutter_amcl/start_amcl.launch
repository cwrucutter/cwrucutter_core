<launch>
  <!-- odom5: .1, odom1: .2, odom2: .2, odom3: .8, odom4:.4 -->
  <node pkg="cutter_amcl" type="cutter_amcl" name="amcl" output="screen">
    <!-- Publish scans from best pose at a max of 10 Hz -->
    <param name="odom_model_type" value="diff"/>
    <param name="odom_alpha5" value="0.4"/>
    <param name="transform_tolerance" value="0.2" />
    <param name="gui_publish_rate" value="10.0"/>
    <param name="min_particles" value="500"/>
    <param name="max_particles" value="5000"/>
    <param name="kld_err" value="0.05"/>
    <param name="kld_z" value="0.99"/>
    <param name="odom_alpha1" value="0.4"/>
    <param name="odom_alpha2" value="0.4"/>
    <!-- translation std dev, m -->
    <param name="odom_alpha3" value="1"/>
    <param name="odom_alpha4" value=".4"/>
    <param name="gps_sigma" value="0.06" />
    <param name="update_min_d" value="0.05"/>
    <param name="update_min_a" value="0.1"/>
    <param name="odom_frame_id" value="odom"/>
    <param name="resample_interval" value="1"/>
    <param name="transform_tolerance" value="0.1"/>
    <param name="recovery_alpha_slow" value="0.001"/>
    <param name="recovery_alpha_fast" value="0.1"/>
  </node>
</launch>
